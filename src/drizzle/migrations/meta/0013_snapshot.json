  "views": {
    "public.countries_with_stats": {
      "columns": {
        "code": {
          "name": "code",
          "type": "text",
          "primaryKey": false,
          "notNull": true
        },
        "name": {
          "name": "name",
          "type": "text",
          "primaryKey": false,
          "notNull": true
        },
        "numberOfProfessionals": {
          "name": "numberOfProfessionals",
          "type": "integer",
          "primaryKey": false,
          "notNull": true,
          "default": 0
        }
      },
      "definition": "\n    SELECT \n      c.code,\n      c.name,\n      COUNT(p.id) as \"numberOfProfessionals\"\n    FROM countries c\n    LEFT JOIN professionals p ON c.code = p.country\n    GROUP BY c.code, c.name\n  ",
      "name": "countries_with_stats",
      "schema": "public",
      "isExisting": false,
      "materialized": false
    },
    "public.professionals_with_stats": {
      "columns": {
        "id": {
          "name": "id",
          "type": "text",
          "primaryKey": false,
          "notNull": true
        },
        "name": {
          "name": "name",
          "type": "text",
          "primaryKey": false,
