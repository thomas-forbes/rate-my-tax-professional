        },
        "rating": {
          "name": "rating",
          "type": "numeric",
          "primaryKey": false,
          "notNull": false
        },
        "fromIrs": {
          "name": "fromIrs",
          "type": "boolean",
          "primaryKey": false,
          "notNull": true,
          "default": false
        },
        "reviewCount": {
          "name": "reviewCount",
          "type": "integer",
          "primaryKey": false,
          "notNull": true,
          "default": 0
        },
        "useAgainPercent": {
          "name": "useAgainPercent",
          "type": "numeric",
          "primaryKey": false,
          "notNull": false
        }
      },
      "definition": "\n    SELECT \n      p.id,\n      p.name,\n      p.credential,\n      p.address,\n      p.country,\n      p.\"fromIrs\",\n      COALESCE(AVG(r.\"overallRating\")::numeric(10, 2), 0) as rating,\n      COUNT(r.id) as \"reviewCount\",\n      CASE \n        WHEN COUNT(r.id) > 0 \n        THEN (SUM(CASE WHEN r.\"useAgain\" THEN 1 ELSE 0 END)::float / COUNT(r.id) * 100)::numeric(10,2)\n        ELSE 0 \n      END as \"useAgainPercent\"\n    FROM professionals p\n    LEFT JOIN reviews r ON p.id = r.\"professionalId\"\n    GROUP BY p.id, p.name, p.credential, p.address, p.country, p.\"fromIrs\"\n  ",
      "name": "professionals_with_stats",
      "schema": "public",
      "isExisting": false,
      "materialized": false
    }
  },
